<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources" type="Fakturowanie.client.resources.AppResources" />

	<g:HTMLPanel styleName="{resources.style.menu_html_Panel}">
		<g:VerticalPanel ui:field="verticalPanel"
			styleName="{resources.style.menu_vertical_panel}">

			<g:Button text="WYSWIETL KLIENTOW" ui:field="buttonWyswietlKlientow"
				styleName="{resources.style.menu_button_rozmiary}" />

			<g:Button text="WYSWIETL PRODUKTY I USLUGI" ui:field="buttonWyswietlPozycje"
				styleName="{resources.style.menu_button_rozmiary}" />

			<g:Button text="WYSWIETL FAKTURY" ui:field="buttonWyswietlFaktury"
				styleName="{resources.style.menu_button_rozmiary}" />

			<g:Button text="DODAJ FAKTURE" ui:field="buttonDodajNowaFakture"
				styleName="{resources.style.menu_button_rozmiary}" />
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>